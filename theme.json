{
    "version": 3,
    "$schema": "https://schemas.wp.org/wp/6.6/theme.json",
    "title": "UFS 11",
    "settings": {
        "appearanceTools": true,
        "color": {
            "defaultPalette": false, 
            "palette": [
                {
                    "color": "#ED008C", 
                    "name": "Primario", 
                    "slug": "primary"
                }, 
                {
                    "color": "#fff", 
                    "name": "White", 
                    "slug": "white"
                }, 
                {
                    "color": "#000", 
                    "name": "Black", 
                    "slug": "black"
                }
            ]
        },
        "typography": {
            "fontFamilies": [
                {
                    "fontFace": [
                        {
                            "fontFamily": "Albert Sans",
                            "fontStyle": "normal",
                            "fontWeight": "100", 
                            "src": [
                                "file:./assets/fonts/albert-sans/AlbertSans-Thin.woff",
                                "file:./assets/fonts/albert-sans/AlbertSans-Thin.woff2"
                            ]
                        },
                        {
                            "fontFamily": "Albert Sans",
                            "fontStyle": "normal",
                            "fontWeight": "400", 
                            "src": [
                                "file:./assets/fonts/albert-sans/AlbertSans-Regular.woff",
                                "file:./assets/fonts/albert-sans/AlbertSans-Regular.woff2"
                            ]
                        },
                        {
                            "fontFamily": "Albert Sans",
                            "fontStyle": "normal",
                            "fontWeight": "500", 
                            "src": [
                                "file:./assets/fonts/albert-sans/AlbertSans-Medium.woff",
                                "file:./assets/fonts/albert-sans/AlbertSans-Medium.woff2"
                            ]
                        },
                        {
                            "fontFamily": "Albert Sans",
                            "fontStyle": "normal",
                            "fontWeight": "700", 
                            "src": [
                                "file:./assets/fonts/albert-sans/AlbertSans-Bold.woff",
                                "file:./assets/fonts/albert-sans/AlbertSans-Bold.woff2"
                            ]
                        }
                    ], 
                    "fontFamily": "Albert Sans",
                    "name": "Albert Sans",
                    "slug": "albertsans"
                }
            ],
            "fluid": true,
            "fontSizes": [
                {
                    "fluid": {
                        "max":"16",
                        "min":"12"
                    },
                    "name":"Piccolo",
                    "size":"16px",
                    "slug":"small"
                },
                {
                    "fluid": {
                        "max":"24",
                        "min":"18"
                    },
                    "name":"Medio",
                    "size":"24px",
                    "slug":"medium"
                },
                {
                    "fluid": {
                        "max":"32",
                        "min":"26"
                    },
                    "name":"Grande",
                    "size":"32px",
                    "slug":"big"
                }
               
            ]
        },
        "background": {
            "backgroundImage": true,
            "backgroundSize": true
        },
        "spacing": {
            "units": [
                "px",
                "em",
                "rem",
                "vh",
                "vw",
                "%"
            ],
            "margin":true,
            "padding": true,
            "defaultSpacingSizes": false,
            "spacingSizes": [
                {
                    "name": "XS",
                    "size": "clamp(1rem, 0.75rem + 0.6667vw, 1.25rem)",
                    "slug": "10"
                },
                {
                    "name": "S",
                    "size": "clamp(1.25rem, 1rem + 0.6667vw, 1.5rem)",
                    "slug": "20"
                },
                {
                    "name": "M",
                    "size": "clamp(1.5rem, 1.25rem + 0.6667vw, 1.75rem)",
                    "slug": "30"
                },
                {
                    "name": "L",
                    "size": "clamp(1.75rem, 1.5rem + 0.6667vw, 2rem)",
                    "slug": "40"
                },
                {
                    "name": "XL",
                    "size": "clamp(2rem, 1.75rem + 0.6667vw, 2.25rem)",
                    "slug": "50"
                },
                {
                    "name": "XXL",
                    "size": "clamp(2.25rem, 2rem + 0.6667vw, 2.5rem)",
                    "slug": "60"
                }

            ]
        }
    }, 
    "styles": {
        "typography": {
			"fontFamily": "var(--wp--preset--font-family--albertsans)",
			"fontStyle": "normal",
			"fontWeight": "400"
		}
    }
}